{
  "name": "ng2-gravatar",
  "version": "0.0.1",
  "description": "Gravatar directive for Angular 2",
  "scripts": {
    "deploy": "NODE_ENV=production webpack -p --progress --color --optimize-minimize --optimize-dedupe --optimize-occurence-order",
    "prepublish": "gulp clean && tsc",
    "prestart": "npm install",
    "server": "webpack-dev-server --hot --inline --colors --display-error-details --display-cached",
    "start": "npm run server",
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/juristr/ng2-gravatar.git"
  },
  "keywords": [
    "Angular2",
    "Angular",
    "Gravatar"
  ],
  "author": "Juri Strumpflohner <juri.strumpflohner@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/juristr/ng2-gravatar/issues"
  },
  "homepage": "https://github.com/juristr/ng2-gravatar#readme",
  "dependencies": {
    "angular2": "2.0.0-alpha.48",
    "reflect-metadata": "0.1.2",
    "rxjs": "5.0.0-alpha.12",
    "zone.js": "0.5.8"
  },
  "devDependencies": {
    "clean-webpack-plugin": "0.1.4",
    "compression-webpack-plugin": "0.2.0",
    "eslint": "1.10.3",
    "exports-loader": "0.6.2",
    "file-loader": "0.8.5",
    "html-loader": "0.4.0",
    "markdown-loader": "0.1.7",
    "marked": "0.3.5",
    "pre-commit": "1.1.2",
    "prismjs": "valorkin/prism",
    "prismjs-loader": "0.0.2",
    "raw-loader": "0.5.1",
    "require-dir": "0.3.0",
    "typescript": "1.7.3",
    "ts-loader": "0.7.2",
    "webpack": "1.12.9",
    "webpack-dev-server": "1.14.0"
  }
}
